{
    "semi": true,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 80,
    "endOfLine": "lf",
    "overrides": [
      {
        "files": "*.js",
        "options": {
          "singleQuote": true
        }
      },
      {
        "files": "*.jsx",
        "options": {
          "printWidth": 100
        }
      },
      {
        "files": "*.html",
        "options": {
          "printWidth": 120
        }
      }
    ]
  }
  